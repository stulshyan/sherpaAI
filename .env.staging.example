# Entropy Platform - Staging Environment Configuration
# Copy this file to .env.staging and fill in your values
# This file is used by the CI/CD pipeline for staging deployments

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=
AWS_ROLE_ARN=

# Database (PostgreSQL - Staging)
STAGING_DATABASE_URL=postgresql://entropy:password@staging-db.example.com:5432/entropy_staging

# Redis (Staging)
STAGING_REDIS_URL=redis://staging-redis.example.com:6379

# Model API Keys
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_API_KEY=

# Slack notifications (optional)
SLACK_WEBHOOK_URL=
